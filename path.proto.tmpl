syntax = "proto3";
package proto.abstracts;

message {{ .ProtoMessageName }} {
{{- range .Enums}}
  enum {{.Name}} {
  {{- range $idx, $option := .Options}}
    {{$option}} = {{$idx}};
  {{- end}}
  }
{{- end}}

  message Biopsy {
  {{- range $idx, $field := .BiopsyFields}}
    {{.FieldType}} {{.Name}} = {{inc $idx}};
  {{- end}}
  }

  message Resection {
  {{- range $idx, $field := .ResectionFields}}
    {{.FieldType}} {{.Name}} = {{inc $idx}};
  {{- end}}
  }
}
